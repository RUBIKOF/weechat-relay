{
  "name": "WeeChat + Relay",
  "version": "0.1.0",
  "slug": "weechat_relay",
  "description": "WeeChat headless IRC client with relay for GlowingBear / Android.",
  "url": "https://weechat.org",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "ports": {
    "9001/tcp": 9001
  },
  "ports_description": {
    "9001/tcp": "WeeChat Relay port"
  },
  "map": [
    "config"
  ],
  "options": {
    "relay_port": 9001,
    "relay_password": "cambia-esta-contrasena"
  },
  "schema": {
    "relay_port": "int",
    "relay_password": "str"
  }
}
